               if (dis1 > dis2 && dis1 > dis3 && dis3 > dis2) {
                   int a = dis1 - dis3 ;
                   int b = dis3 - dis2;
 
 
                            System.out.println(a + b);
 
                   break;
               }
 
                else if (dis1 > dis2 && dis1 > dis3 && dis3 < dis2) {
                   int m = dis2 - dis3 ;
                   int n = dis1 - dis2;
 
 
                       System.out.println(m + n);
 
                   break;
 
               }
 
